{
    "name": "lexique-quebecois",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "next dev",
        "serve:https": "local-ssl-proxy --source 3030 --target 3000",
        "build": "pnpm run build:site && pnpm run build:sitemap",
        "build:site": "next build",
        "build:sitemap": "next-sitemap --config next-sitemap.config.js",
        "preview": "pnpm run build:site && next start",
        "lint": "next lint",
        "test": "jest",
        "test:verbose": "jest --verbose",
        "test:coverage": "jest --coverage",
        "e2e": "playwright test",
        "e2e:report": "playwright show-report",
        "analyze:bundle": "cross-env ANALYZE_BUNDLE=true next build",
        "analyze:source-map": "cross-env ANALYZE_SOURCE_MAP=true next build && source-map-explorer .next/static/**/*.js"
    },
    "dependencies": {
        "@amplitude/analytics-browser": "^1.10.3",
        "@babel/core": "^7.21.8",
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@hcaptcha/react-hcaptcha": "^1.8.1",
        "@hookform/resolvers": "^3.1.0",
        "@mui/icons-material": "^5.11.16",
        "@mui/lab": "5.0.0-alpha.129",
        "@mui/material": "^5.13.0",
        "@next-auth/mongodb-adapter": "^1.1.3",
        "@next/env": "^13.4.1",
        "mongodb": "^5.4.0",
        "next": "^13.4.1",
        "next-auth": "^4.22.1",
        "nodemailer": "^6.9.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.9",
        "react-tracking": "^9.3.1",
        "request-ip": "^3.3.0",
        "seedrandom": "^3.0.5",
        "swr": "^2.1.5",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^13.4.1",
        "@playwright/test": "^1.33.0",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.15.3",
        "@types/nodemailer": "^6.4.7",
        "@types/react": "^18.2.6",
        "@types/react-tracking": "^8.1.3",
        "@types/request-ip": "^0.0.37",
        "@types/seedrandom": "^3.0.5",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.40.0",
        "eslint-config-next": "^13.4.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "local-ssl-proxy": "^2.0.5",
        "next-sitemap": "^4.0.9",
        "source-map-explorer": "^2.5.3",
        "typescript": "^5.0.4"
    }
}