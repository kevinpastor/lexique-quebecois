{
    "name": "lexique-quebecois",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "next dev",
        "start:https": "local-ssl-proxy --source 3030 --target 3000",
        "build": "pnpm run build:site && pnpm run build:sitemap",
        "build:site": "next build",
        "build:sitemap": "next-sitemap --config next-sitemap.config.js",
        "preview": "pnpm run build:site && next start",
        "lint": "next lint",
        "test": "jest",
        "test:verbose": "jest --verbose",
        "test:coverage": "jest --coverage",
        "e2e": "playwright test",
        "e2e:report": "playwright show-report",
        "analyze:bundle": "cross-env ANALYZE_BUNDLE=true next build",
        "analyze:source-map": "cross-env ANALYZE_SOURCE_MAP=true next build && source-map-explorer .next/static/**/*.js"
    },
    "dependencies": {
        "@babel/core": "^7.20.12",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@hookform/resolvers": "^2.9.10",
        "@mui/icons-material": "^5.11.0",
        "@mui/lab": "5.0.0-alpha.115",
        "@mui/material": "^5.11.4",
        "@mui/system": "^5.11.4",
        "@next/env": "^13.1.1",
        "mongodb": "^4.13.0",
        "next": "^13.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.41.5",
        "request-ip": "^3.3.0",
        "seedrandom": "^3.0.5",
        "swr": "^1.3.0",
        "tiny-warning": "^1.0.3",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^13.1.1",
        "@playwright/test": "^1.29.2",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.26",
        "@types/request-ip": "^0.0.37",
        "@types/seedrandom": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "babel-jest": "^29.3.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.31.0",
        "eslint-config-next": "^13.1.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "local-ssl-proxy": "^1.3.0",
        "next-sitemap": "^3.1.45",
        "source-map-explorer": "^2.5.3",
        "typescript": "^4.9.4"
    }
}