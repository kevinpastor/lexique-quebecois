{
    "name": "@quebecois-urbain/client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "next dev",
        "start:https": "local-ssl-proxy --source 3030 --target 3000",
        "build": "pnpm run build:site && pnpm run build:sitemap",
        "build:site": "next build",
        "build:sitemap": "next-sitemap --config next-sitemap.config.js",
        "preview": "pnpm run build:site && next start",
        "lint": "next lint",
        "test": "jest",
        "test:verbose": "jest --verbose",
        "test:coverage": "jest --coverage",
        "analyze:bundle": "cross-env ANALYZE_BUNDLE=true next build",
        "analyze:source-map": "cross-env ANALYZE_SOURCE_MAP=true next build && source-map-explorer .next/static/**/*.js"
    },
    "dependencies": {
        "@babel/core": "^7.19.3",
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@mui/icons-material": "^5.10.6",
        "@mui/lab": "5.0.0-alpha.93",
        "@mui/material": "^5.10.7",
        "@next/env": "^12.3.1",
        "classnames": "^2.3.2",
        "formik": "^2.2.9",
        "formik-mui": "5.0.0-alpha.0",
        "mongodb": "^4.10.0",
        "next": "^12.3.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "request-ip": "^3.3.0",
        "seedrandom": "^3.0.5",
        "swr": "^1.3.0",
        "tiny-warning": "^1.0.3",
        "uuid": "^8.3.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.3.1",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^28.1.8",
        "@types/node": "^17.0.45",
        "@types/react": "^18.0.21",
        "@types/request-ip": "^0.0.37",
        "@types/seedrandom": "^3.0.2",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "babel-jest": "^28.1.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.24.0",
        "eslint-config-next": "^12.3.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-promise": "^6.0.1",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "local-ssl-proxy": "^1.3.0",
        "next-sitemap": "^3.1.23",
        "source-map-explorer": "^2.5.3",
        "typescript": "^4.8.4"
    }
}
