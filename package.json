{
    "name": "lexique-quebecois",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "next dev",
        "start:https": "local-ssl-proxy --source 3030 --target 3000",
        "build": "pnpm run build:site && pnpm run build:sitemap",
        "build:site": "next build",
        "build:sitemap": "next-sitemap --config next-sitemap.config.js",
        "preview": "pnpm run build:site && next start",
        "lint": "next lint",
        "test": "jest",
        "test:verbose": "jest --verbose",
        "test:coverage": "jest --coverage",
        "e2e": "playwright test",
        "e2e:report": "playwright show-report",
        "analyze:bundle": "cross-env ANALYZE_BUNDLE=true next build",
        "analyze:source-map": "cross-env ANALYZE_SOURCE_MAP=true next build && source-map-explorer .next/static/**/*.js"
    },
    "dependencies": {
        "@babel/core": "^7.21.0",
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@hcaptcha/react-hcaptcha": "^1.4.4",
        "@hookform/resolvers": "^2.9.11",
        "@mui/icons-material": "^5.11.9",
        "@mui/lab": "5.0.0-alpha.120",
        "@mui/material": "^5.11.10",
        "@next/env": "^13.2.0",
        "mongodb": "^5.1.0",
        "next": "^13.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.2",
        "request-ip": "^3.3.0",
        "seedrandom": "^3.0.5",
        "swr": "^2.0.3",
        "zod": "^3.20.6"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^13.2.0",
        "@playwright/test": "^1.31.1",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.14.1",
        "@types/react": "^18.0.28",
        "@types/request-ip": "^0.0.37",
        "@types/seedrandom": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.34.0",
        "eslint-config-next": "^13.2.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.4.3",
        "jest-environment-jsdom": "^29.4.3",
        "local-ssl-proxy": "^1.3.0",
        "next-sitemap": "^3.1.52",
        "source-map-explorer": "^2.5.3",
        "typescript": "^4.9.5"
    }
}