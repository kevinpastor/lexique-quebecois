{
  "name": "lexique-quebecois",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^18.17.0",
    "pnpm": "^8.10.0"
  },
  "scripts": {
    "start": "next dev --turbo",
    "serve:https": "local-ssl-proxy --source 3030 --target 3000",
    "build": "next build",
    "preview": "next build && next start",
    "lint": "next lint",
    "test": "pnpm run test:unit",
    "test:unit": "jest",
    "test:unit:verbose": "jest --verbose",
    "test:unit:coverage": "jest --coverage",
    "test:unit:coverage:watch": "jest --coverage --watchAll",
    "test:mutation": "stryker run",
    "e2e": "pnpm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "analyze": "cross-env ANALYZE=true next build && source-map-explorer .next/static/**/*.js && next start"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@ctrl/react-adsense": "^1.7.0",
    "@emotion/cache": "^11.11.0",
