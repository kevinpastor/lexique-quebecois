{
    "name": "@quebecois-urbain/client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "next dev",
        "build": "pnpm run build:site && pnpm run build:sitemap",
        "build:site": "next build",
        "build:sitemap": "next-sitemap --config next-sitemap.config.js",
        "preview": "pnpm run build:site && next start",
        "lint": "next lint",
        "test": "jest",
        "test:verbose": "jest --verbose",
        "test:coverage": "jest --coverage",
        "analyze:bundle": "cross-env ANALYZE_BUNDLE=true next build",
        "analyze:source-map": "cross-env ANALYZE_SOURCE_MAP=true next build && source-map-explorer .next/static/**/*.js"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/lab": "5.0.0-alpha.93",
        "@mui/material": "^5.9.1",
        "classnames": "^2.3.1",
        "formik": "^2.2.9",
        "formik-mui": "5.0.0-alpha.0",
        "mongodb": "^4.5.0",
        "next": "^12.2.4",
        "notistack": "^2.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "request-ip": "^3.3.0",
        "seedrandom": "^3.0.5",
        "swr": "^1.3.0",
        "usehooks-ts": "^2.6.0",
        "uuid": "^8.3.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.2.3",
        "@testing-library/react": "^13.2.0",
        "@types/jest": "^28.1.1",
        "@types/node": "^17.0.21",
        "@types/react": "^18.0.8",
        "@types/request-ip": "^0.0.37",
        "@types/seedrandom": "^3.0.2",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "babel-jest": "^28.0.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.8.0",
        "eslint-config-next": "^12.0.10",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^28.0.3",
        "jest-environment-jsdom": "^28.0.2",
        "next-sitemap": "^3.0.5",
        "source-map-explorer": "^2.5.2",
        "typescript": "^4.5.5"
    }
}
