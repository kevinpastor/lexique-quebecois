{
    "name": "lexique-quebecois",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "^20.0.0",
        "pnpm": "^9.4.0"
    },
    "packageManager": "pnpm@9.4.0",
    "scripts": {
        "start": "next dev --turbo",
        "start:profile": "cross-env PROFILE=true next dev",
        "serve:https": "local-ssl-proxy --source 3030 --target 3000",
        "build": "next build",
        "preview": "next build && next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:mutation": "stryker run",
        "e2e": "playwright test",
        "e2e:report": "playwright show-report",
        "analyze": "cross-env ANALYZE=true next build && source-map-explorer .next/static/**/*.js && next start",
        "preinstall": "npx only-allow pnpm"
    },
    "dependencies": {
        "@babel/core": "^7.25.2",
        "@ctrl/react-adsense": "^1.7.0",
        "@emotion/cache": "^11.13.1",
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@hcaptcha/react-hcaptcha": "^1.11.0",
        "@hookform/resolvers": "^3.9.0",
        "@mui/icons-material": "^6.0.0",
        "@mui/lab": "6.0.0-beta.8",
        "@mui/material": "^6.0.0",
        "@mui/material-nextjs": "^6.0.0",
        "@next/env": "^14.2.7",
        "@vercel/analytics": "^1.3.1",
        "@vercel/speed-insights": "^1.0.12",
        "@vitest/coverage-istanbul": "^2.0.5",
        "dataloader": "^2.2.2",
        "framer-motion": "^11.3.30",
        "mongodb": "^6.8.0",
        "next": "^14.2.7",
        "nodemailer": "^6.9.14",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-helmet-async": "^2.0.5",
        "react-hook-form": "^7.53.0",
        "request-ip": "^3.3.0",
        "seedrandom": "^3.0.5",
        "sharp": "^0.33.5",
        "swr": "^2.2.5",
        "valibot": "^0.36.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^14.2.7",
        "@playwright/test": "^1.46.1",
        "@stryker-mutator/api": "^8.5.0",
        "@stryker-mutator/core": "^8.5.0",
        "@stryker-mutator/typescript-checker": "^8.5.0",
        "@stryker-mutator/vitest-runner": "^8.5.0",
        "@stylistic/eslint-plugin": "^1.8.1",
        "@testing-library/react": "^16.0.0",
        "@types/node": "^20.16.1",
        "@types/nodemailer": "^6.4.15",
        "@types/react": "^18.3.4",
        "@types/react-helmet": "^6.1.11",
        "@types/request-ip": "^0.0.41",
        "@types/seedrandom": "^3.0.8",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "@vitejs/plugin-react": "^4.3.1",
        "@vitest/browser": "^2.0.5",
        "@welldone-software/why-did-you-render": "^8.0.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.7",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-promise": "^6.6.0",
        "eslint-plugin-sonarjs": "^0.25.1",
        "local-ssl-proxy": "^2.0.5",
        "source-map-explorer": "^2.5.3",
        "typescript": "^5.5.4",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^2.0.5"
    }
}